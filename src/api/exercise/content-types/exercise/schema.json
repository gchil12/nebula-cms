{
  "kind": "collectionType",
  "collectionName": "exercises",
  "info": {
    "singularName": "exercise",
    "pluralName": "exercises",
    "displayName": "Exercise"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "exerciseFormat": {
      "type": "enumeration",
      "enum": [
        "MULTIPLE CHOICE",
        "OPEN"
      ]
    },
    "probableAnswers": {
      "type": "json"
    },
    "correctAnswer": {
      "type": "string"
    },
    "section": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::section.section",
      "inversedBy": "exercises"
    },
    "lesson": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::lesson.lesson",
      "inversedBy": "exercises"
    },
    "body": {
      "type": "richtext"
    },
    "remediation": {
      "type": "json"
    }
  }
}
